<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 인터페이스 연결 -->
<!-- mapper namespace = "package명.인터페이스명" -->
<mapper namespace="com.gck.post.model.PostImageDAO">
    <insert id="insertPostImage" parameterType="com.gck.post.model.PostImageVO">
        INSERT
        INTO post_images(post_idx, post_image_originalname, post_image_savedname)
        VALUES(#{postIdx}, #{postImageOriginalname}, #{postImageSavedname})
    </insert>
</mapper>